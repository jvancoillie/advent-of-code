{
    "type": "project",
    "license": "proprietary",
    "name": "jvancoillie/advent-of-code",
    "description": "advent of code ",
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/dotenv": "^5.2",
        "ext-json": "*",
        "ext-gmp": "*",
        "symfony/cache": "^6.0",
        "symfony/options-resolver": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2",
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpstan/phpstan": "^1.2",
        "vimeo/psalm": "^4.13",
        "rector/rector": "^0.12.5",
        "phpunit/phpunit": "^9.5"
    }
}
